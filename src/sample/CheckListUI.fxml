    <Label text="Clean" GridPane.columnIndex="0" GridPane.rowIndex="1"></Label>
    <CheckBox id="Clean" GridPane.columnIndex="1" GridPane.rowIndex="1"></CheckBox>

    <Label text="Alignment" GridPane.columnIndex="0" GridPane.rowIndex="2"></Label>
