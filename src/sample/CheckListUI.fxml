    <Label text="Shutter" GridPane.columnIndex="0" GridPane.rowIndex="9"></Label>
    <CheckBox id="Shutter" GridPane.columnIndex="1" GridPane.rowIndex="9"></CheckBox>

    <Label text="Lamp" GridPane.columnIndex="0" GridPane.rowIndex="10"></Label>
